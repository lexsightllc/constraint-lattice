#!/usr/bin/env bash
set -euo pipefail

SCRIPT_DIR=$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)
source "$SCRIPT_DIR/lib.sh"

log INFO "Removing build artifacts and caches"
rm -rf build dist .mypy_cache .pytest_cache .ruff_cache htmlcov coverage.xml
find "$REPO_ROOT" -type d -name "__pycache__" -prune -exec rm -rf {} +
